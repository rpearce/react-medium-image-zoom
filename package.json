{
  "name": "react-medium-image-zoom",
  "version": "4.4.0",
  "description": "Accessible medium.com-style image zoom for React",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:rpearce/image-zoom.git"
  },
  "homepage": "https://github.com/rpearce/image-zoom",
  "bugs": "https://github.com/rpearce/image-zoom/issues",
  "author": "Robert Pearce <me@robertwpearce.com>",
  "license": "BSD-3",
  "keywords": [
    "react",
    "medium",
    "image",
    "zoom",
    "image zoom",
    "modal",
    "react-component"
  ],
  "tags": [
    "react",
    "medium",
    "image",
    "zoom",
    "image zoom",
    "modal",
    "react-component"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "AUTHORS",
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "scripts": {
    "build": "run-s clean build:js build:css build:docs",
    "build:css": "cp ./source/styles.css ./dist/styles.css",
    "build:docs": "mkdir -p docs && build-storybook -o docs",
    "build:js": "rollup -c ./rollup.config.js",
    "build:js:watch": "npm run build:js -- -w",
    "ci": "run-p lint build test",
    "clean": "run-p clean:dist clean:docs",
    "clean:dist": "rm -rf ./dist",
    "clean:docs": "rm -rf ./docs",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "prepublishOnly": "run-p lint test && npm run build:js",
    "start": "start-storybook -p 6006",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@storybook/addon-a11y": "^6.4.22",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/react": "^6.4.22",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/axe-core": "^2.0.7",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "all-contributors-cli": "^6.20.0",
    "axe-core": "^4.4.2",
    "babel-loader": "^8.2.5",
    "coveralls": "^3.1.1",
    "eslint": "^8.15.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rollup": "^2.72.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "focus-options-polyfill": "1.5.1",
    "tslib": "^2.4.0"
  }
}
