    "medium",
    "modal",
    "zoom",
    "image"
  ],
  "scripts": {
    "build": "npm run build:js && npm run build:docs",
    "build:docs": "rm docs/app.js && browserify example/app.js -o docs/app.js -t [ babelify ]",
    "build:js": "rm -rf src && babel source --out-dir src",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint --ext .js .",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "npm run lint && npm run build && jest --coverage --no-cache"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "<rootDir>/source/**/*.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "all-contributors-cli": "^5.4.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "coveralls": "^3.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.11.1",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.0",
    "jest": "^23.6.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "peerDependencies": {
    "react": ">= 16.3",
    "react-dom": ">= 16.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@rpearce/simple-uniqueid": "^0.1.0",
    "react-button-a11y": "^2.0.0"
  }
}
